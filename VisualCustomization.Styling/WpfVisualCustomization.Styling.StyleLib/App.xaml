<Application x:Class="WpfVisualCustomization.Styling.StyleLib.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WpfVisualCustomization.Styling.StyleLib">
    <Application.Resources>
        <Style x:Key="{ComponentResourceKey {x:Type local:DummyClass}, myComponentLibAppStyle}" TargetType="Button">
            <Setter Property="Background" Value="#FFB8B838" />
            <Setter Property="BorderBrush" Value="#FF32B6A4" />
        </Style>
    </Application.Resources>
</Application>
